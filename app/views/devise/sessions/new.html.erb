
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>


  <div class="body"></div>

<div class="fixed_form">
		<div class="header">
 <%=  image_tag("logo.png") %>		</div>
		<br>
		<div class="login">
    <%= f.email_field :email, autofocus: true, placeholder: "Email"%><br/ >
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %><br/ >


      
    <%= f.submit "Log in" %> 
  
    <%= render "devise/shared/links" %>
    
		</div>
    
</div>    

  



<% end %>

