
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>


  <div class="body"></div>


		<div class="header">
<%=  image_tag("logo.png") %>		</div>
		<br>
		<div class="login">
    <%= f.email_field :email, autofocus: true, placeholder: "Email"%><br/ >
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %><br/ >


      
    <%= f.submit "Log in" %> 
    <%= link_to "Sign Up", new_user_registration_path, class: "sign_up"%>
		</div>
    
    

  



<% end %>

<%= render "devise/shared/links" %>
