<!DOCTYPE html>
<html>
<head>
  <title>Ussf</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
 <div id="wrapper">

<%= render 'layouts/nav' %>


<div id="page-wrapper">
  <% if notice %>
    <p class="alert alert-success"><%= notice %></p>
  <% end %>
  <% if alert %>
    <p class="alert alert-danger"><%= alert %></p>
  <% end %>
<%= yield %>
</div>
</body>
</html>
